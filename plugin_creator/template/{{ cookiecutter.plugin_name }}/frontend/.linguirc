{
    "locales": [
        "de",
        "en",
        "es",
        "fr",
        "it",
        "ja",
        "ru",
        "zh_Hans",
        "zh_Hant",
        "p"
    ],
    "catalogs": [{
        "path": "src/locales/{locale}/messages",
        "include": ["src"],
        "exclude": ["**/node_modules/**", "./dist/**"]
    }],
    "format": "po",
    "orderBy": "origin",
    "sourceLocale": "en",
    "fallbackLocales": {
        "default": "en"
    }
}